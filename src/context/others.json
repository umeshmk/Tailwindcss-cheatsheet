{
  "spacing": {
    "padding": {
      "col": 1,
      "data": ["p-[$SPACING]", "p[x|y]-[$SPACING]", "p[t|r|b|l]-[$SPACING]"]
    },
    "margin": {
      "col": 1,
      "data": [
        "m-auto",
        "-m-[$SPACING]",
        "-m[x|y]-[$SPACING]",
        "-m[t|r|b|l]-[$SPACING]",
        "m-[$SPACING]",
        "m[x|y]-[$SPACING]",
        "m[t|r|b|l]-[$SPACING]"
      ]
    },
    "space Between": {
      "col": 1,
      "data": [
        "-space-[x|y]-[$SPACING]",
        "space-[x|y]-[$SPACING]",
        "space-[x|y]-reverse"
      ]
    }
  },

  "sizing": {
    "[ width|height ]": {
      "col": 1,
      "data": [
        "[w|h]-[$SPACING]",
        "[w|h]-[auto|full|screen|fit]",
        "[w|h]-[min|max]",
        "[w|h]-1/2",
        "[w|h]-{1-2}/3",
        "[w|h]-{1-3}/4",
        "[w|h]-{1-4}/5",
        "[w|h]-{1-5}/6",
        "w-{1-11}/12"
      ]
    },
    "min-[ width|height ]": {
      "col": 1,
      "data": [
        "min-h-screen",
        "min-[w|h]-0",
        "min-[w|h]-[full|fit]",
        "min-[w|h]-[min|max]"
      ]
    },
    "max-width": {
      "col": 2,
      "data": [
        "max-w-0",
        "max-w-none",
        "max-w-[xs|sm|md|lg|xl]",
        "max-w-{2-7}xl",
        "max-w-prose",
        "max-w-screen-[xs|sm|md]",
        "max-w-screen-[lg|xl|2xl]",
        "max-w-[full|fit]",
        "max-w-[min|max]"
      ]
    },
    "max-height": {
      "col": 2,
      "data": ["max-h-[full|screen|fit]", "max-h-[min|max]", "max-h-[$SPACING]"]
    }
  },
  "effect": {
    "Box-Shadow": {
      "col": 2,
      "data": [
        "shadow",
        "shadow-[sm|md|lg|xl|2xl]",
        "shadow-inner",
        "shadow-none"
      ]
    },
    "Box-Shadow-Color": {
      "col": 2,
      "data": [
        "shadow--transparent",
        "shadow--{inherit|current}",
        "shadow--white",
        "shadow--black",
        "shadow--{$color}-{50-900}"
      ]
    },
    "Opacity": {"col": 2, "data": ["opacity-[$OPACITY]"]},
    "[Mix|Background]-Blend-Mode": {
      "col": 1,
      "data": [
        "[mix|bg]-blend-normal",
        "[mix|bg]-blend-multiply",
        "[mix|bg]-blend-screen",
        "[mix|bg]-blend-overlay",
        "[mix|bg]-blend-{darken|lighten}",
        "[mix|bg]-blend-color",
        "[mix|bg]-blend-color-{dodge|burn}",
        "[mix|bg]-blend-{soft|hard}-light",
        "[mix|bg]-blend-difference",
        "[mix|bg]-blend-exclusion",
        "[mix|bg]-blend-[hue|saturation]",
        "[mix|bg]-blend-luminosity",
        "[mix|bg]-blend-plus-lighter"
      ]
    }
  },

  "table": {
    "border-collapse": {
      "col": 1,
      "data": ["border-collapse", "border-separate"]
    },
    "border-spacing": {
      "col": 1,
      "data": ["border-spacing-[$SPACING]", "border-spacing-[x|y]-[$SPACING]"]
    },
    "Table-Layout": {"col": 1, "data": ["table-auto", "table-fixed"]}
  },
  "transition": {
    "property": {
      "col": 1,
      "data": [
        "transition-none",
        "transition-all",
        "transition",
        "transition-colors",
        "transition-opacity",
        "transition-shadow",
        "transition-transform"
      ]
    },
    "[duration|delay]": {
      "col": 1,
      "data": [
        "[duration|delay]-[75|100|150|200]",
        "[duration|delay]-[300|500|700|1000]"
      ]
    },
    "timing function": {
      "col": 2,
      "data": ["ease-linear", "ease-in", "ease-out", "ease-in-out"]
    },
    "animation": {
      "col": 2,
      "data": [
        "animate-none",
        "animate-spin",
        "animate-ping",
        "animate-pulse",
        "animate-bounce"
      ]
    }
  },

  "transform": {
    "transform-origin": {
      "col": 1,
      "data": [
        "origin-[top|right|bottom|left]",
        "origin-[top|bottom]-[right|left]",
        "origin-center"
      ]
    },
    "scale": {
      "col": 1,
      "data": [
        "scale-[0|50|75|90|95|100]",
        "scale-[105|110|125|150]",
        "scale-[x|y]-[0|50|75|90|95|100]",
        "scale-[x|y]-[105|110|125|150]"
      ]
    },
    "rotate": {
      "col": 1,
      "data": [
        "rotate-[0.1.2.3.6.12.45.90.180]",
        "-rotate-[1.2.3.6.12.45.90.180]"
      ]
    },
    "translate": {
      "col": 2,
      "data": [
        "{-}translate-[x|y]-[$spacing]",
        "{-}translate-[x|y]-1/2",
        "{-}translate-[x|y]-[1|2]/3",
        "{-}translate-[x|y]-[1|2|3]/4",
        "{-}translate-[x|y]-[full|]"
      ]
    },
    "skew": {"col": 2, "data": ["{-}skew-[x|y]-[0.1.2.3.6.12]"]}
  },

  "svg": {
    "[fill|stroke]": {
      "col": 1,
      "data": [
        "[fill|stroke]-none",
        "[fill|stroke]-transparent",
        "[fill|stroke]-[inherit|current]",
        "[fill|stroke]-[white|black]",
        "[fill|stroke]-[$color]-[50-900]"
      ]
    },
    "stroke-width": {"col": 1, "data": ["stroke-[0|1|2]"]}
  },

  "accessibility": {
    "screen-reader": {"col": 1, "data": ["sr-only", "not-sr-only"]}
  }
}
